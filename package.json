{
  "name": "tractor",
  "version": "0.11.0-beta.1",
  "author": "Craig Spence <craig@trademe.co.nz>",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craig@trademe.co.nz"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "3.5.0",
    "coveralls": "^2.13.0",
    "dirty-chai": "1.2.2",
    "eslint": "^3.19.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "npm-watch": "^0.1.8",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "graceful-fs": "^4.1.11",
    "tractor-config-loader": "^0.3.1",
    "tractor-dependency-injection": "^0.1.1",
    "tractor-error-handler": "^0.2.0",
    "tractor-logger": "^0.1.0",
    "tractor-plugin-loader": "^0.4.0",
    "tractor-server": "^0.11.0-beta.0"
  },
  "bin": {
    "tractor": "./bin/tractor"
  }
}
