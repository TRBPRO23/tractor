{
  "name": "@tractor/cli",
  "version": "1.0.1-beta.10",
  "description": "CLI for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "bin": {
    "tractor": "./bin/tractor"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "nyc npm run test",
    "lint": "eslint src --ignore-path ../../.eslintignore",
    "test": "cd ../../ && mocha --opts mocha.opts 'packages/cli/src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "@tractor/core": "^1.0.1-beta.10",
    "@babel/polyfill": "^7.0.0",
    "bluebird": "^3.5.1",
    "commander": "^2.13.0",
    "protractor": "^5.3.0"
  },
  "devDependencies": {
    "@tractor/config-loader": "^1.0.1-beta.10",
    "@tractor/dependency-injection": "^1.0.1-beta.10",
    "@tractor/error-handler": "^1.0.1-beta.10",
    "@tractor/file-structure": "^1.0.1-beta.10",
    "@tractor/logger": "^1.0.1-beta.10",
    "@tractor/plugin-loader": "^1.0.1-beta.10",
    "@tractor/server": "^1.0.1-beta.10"
  },
  "gitHead": "75f58824a7a14b8012a99e91ed23a80ab090e093"
}
