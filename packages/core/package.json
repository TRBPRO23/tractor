{
  "name": "@tractor/core",
  "version": "1.0.8",
  "description": "Core module for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile && npm run cover",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist --ignore src/**/*.spec.js --config-file ../../babel.config.js",
    "cover": "nyc npm run test",
    "lint": "eslint src --ignore-path ../../.eslintignore",
    "test": "cd ../../ && mocha --opts mocha.opts 'packages/core/src/**/*.spec.js' && cd ./packages/core",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "@tractor/config-loader": "^1.0.7",
    "@tractor/dependency-injection": "^1.0.6",
    "@tractor/error-handler": "^1.0.7",
    "@tractor/file-javascript": "^1.0.6",
    "@tractor/file-structure": "^1.0.6",
    "@tractor/logger": "^1.0.7",
    "@tractor/plugin-loader": "^1.0.8",
    "@tractor/server": "^1.0.6",
    "@tractor/ui": "^1.0.2",
    "callsite": "^1.0.0"
  },
  "gitHead": "75f58824a7a14b8012a99e91ed23a80ab090e093"
}
