{
  "name": "@tractor/plugin-loader",
  "version": "1.0.0",
  "description": "File structure handler for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile && npm run cover",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist --ignore src/**/*.spec.js --config-file ../../babel.config.js",
    "cover": "nyc npm run test",
    "lint": "eslint src --ignore-path ../../.eslintignore",
    "test": "cd ../../ && mocha --opts mocha.opts 'packages/plugin-loader/src/**/*.spec.js' && cd ./packages/plugin-loader",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "camel-case": "^3.0.0",
    "find-up": "^3.0.0",
    "param-case": "^2.1.1",
    "pkg-up": "^2.0.0",
    "resolve-from": "^4.0.0",
    "title-case": "^2.1.1"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.0"
  },
  "gitHead": "75f58824a7a14b8012a99e91ed23a80ab090e093"
}
