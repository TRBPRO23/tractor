{
  "name": "@tractor/server",
  "version": "1.0.1-beta.10",
  "description": "Server for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "nyc npm run test",
    "lint": "eslint src --ignore-path ../../.eslintignore",
    "test": "cd ../../ && mocha --opts mocha.opts 'packages/server/src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "js-search": "^1.4.2",
    "lodash.debounce": "^4.0.8",
    "lodash.template": "^4.4.0",
    "monkeypatch": "^1.0.0",
    "porter-stemmer": "^0.9.1",
    "socket.io": "^2.0.4"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.1-beta.8"
  },
  "devDependencies": {
    "@tractor/error-handler": "^1.0.1-beta.10",
    "@tractor/file-structure": "^1.0.1-beta.10",
    "@tractor/logger": "^1.0.1-beta.10",
    "@tractor/ui": "^1.0.1-beta.10"
  },
  "gitHead": "75f58824a7a14b8012a99e91ed23a80ab090e093"
}
