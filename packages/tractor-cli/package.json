{
  "name": "@tractor/cli",
  "version": "1.0.1-beta.9",
  "description": "CLI for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "bin": {
    "tractor": "./bin/tractor"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "@tractor/core": "^1.0.1-beta.9",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "commander": "^2.13.0",
    "protractor": "^5.3.0"
  },
  "devDependencies": {
    "@tractor/config-loader": "^1.0.1-beta.9",
    "@tractor/dependency-injection": "^1.0.1-beta.9",
    "@tractor/error-handler": "^1.0.1-beta.9",
    "@tractor/file-structure": "^1.0.1-beta.9",
    "@tractor/logger": "^1.0.1-beta.9",
    "@tractor/plugin-loader": "^1.0.1-beta.9",
    "@tractor/server": "^1.0.1-beta.9"
  },
  "gitHead": "5692dee7a04173c9362b7122217b685d3c81029d"
}
