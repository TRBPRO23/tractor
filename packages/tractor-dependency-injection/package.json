{
  "name": "tractor-dependency-injection",
  "version": "0.1.3",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-dependency-injection.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-dependency-injection/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "devDependencies": {
    "tractor-error-handler": "^0.2.4",
    "tractor-logger": "^0.1.1"
  },
  "peerDependencies": {
    "tractor": "^0.12.1"
  }
}
