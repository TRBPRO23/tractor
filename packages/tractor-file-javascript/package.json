{
  "name": "tractor-file-javascript",
  "version": "0.2.6",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-file-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-file-javascript/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "devDependencies": {
    "tractor": "^0.12.9",
    "tractor-error-handler": "^0.2.4",
    "tractor-file-structure": "^0.5.7",
    "tractor-logger": "^0.1.1"
  },
  "dependencies": {
    "escodegen": "^1.9.0",
    "esprima": "^4.0.0",
    "esquery": "^1.0.0"
  },
  "peerDependencies": {
    "tractor": "^0.12.1"
  }
}
