{
  "name": "@tractor/server",
  "version": "1.0.1-beta.6",
  "description": "Server for tractor",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.1",
    "express": "^4.16.2",
    "js-search": "^1.4.2",
    "lodash.debounce": "^4.0.8",
    "lodash.template": "^4.4.0",
    "monkeypatch": "^1.0.0",
    "porter-stemmer": "^0.9.1",
    "socket.io": "^2.0.4"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.1-beta.6"
  },
  "devDependencies": {
    "@tractor/error-handler": "^1.0.1-beta.6",
    "@tractor/file-structure": "^1.0.1-beta.6",
    "@tractor/logger": "^1.0.1-beta.6",
    "@tractor/ui": "^1.0.1-beta.6"
  }
}
