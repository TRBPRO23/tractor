{
  "name": "@tractor/unit-test",
  "version": "1.0.1-beta.8",
  "description": "Unit test utilities for the tractor monorepo",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "dedent": "^0.7.0",
    "dirty-chai": "^2.0.1",
    "ineeda": "^0.11.1",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.1-beta.8"
  },
  "gitHead": "a3966ff57d645b11b2e99ee784cf97e8141bb189"
}
