{
  "name": "tractor",
  "version": "0.12.10",
  "author": "Craig Spence <craig@trademe.co.nz>",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craig@trademe.co.nz"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist && rimraf reports",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config ../../.istanbulrc.json ../../node_modules/.bin/_mocha -- --opts ../../.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src",
    "test": "mocha --opts ../../.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "commander": "^2.13.0",
    "protractor": "^5.3.0",
    "tractor-client": "^0.12.4",
    "tractor-config-loader": "^0.5.0",
    "tractor-dependency-injection": "^0.1.3",
    "tractor-error-handler": "^0.2.4",
    "tractor-file-structure": "^0.5.7",
    "tractor-logger": "^0.1.1",
    "tractor-plugin-loader": "^0.6.4",
    "tractor-server": "^0.12.6"
  },
  "bin": {
    "tractor": "./bin/tractor"
  }
}
