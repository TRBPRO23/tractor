{
  "name": "@tractor-plugins/mock-requests",
  "version": "0.4.3",
  "description": "tractor plugin for mocking HTTP/Fetch requests",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run cover && npm run compile && npm run bundle:server && npm run bundle:client",
    "bundle:client": "webpack --config webpack.config.js",
    "bundle:server": "webpack --config webpack.server.config.js",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist/ --ignore *.spec.js,src/tractor/server/scripts,src/tractor/client --config-file ../../babel.config.js",
    "cover": "nyc npm run test",
    "lint": "eslint src --ignore-path ../../.eslintignore",
    "test": "cd ../../ && mocha --opts mocha.opts 'plugins/mock-requests/src/**/*.spec.js' && cd ./plugins/mock-requests",
    "pretest:e2e": "npm run test:setup",
    "test:e2e": "protractor ./tractor/protractor.conf.js --baseUrl=\"http://localhost:4321\"",
    "test:e2e:debug": "node --inspect-brk node_modules/.bin/protractor ./tractor/protractor.conf.js --baseUrl=\"http://localhost:4321\"",
    "test:setup": "rimraf test && mkdirp test && npm run webdriver",
    "tractor": "tractor start",
    "tractor:init": "tractor init",
    "pretractor:test": "npm run test:setup",
    "tractor:test": "tractor start --config=\"./test.tractor.conf.js\"",
    "watch": "npm-watch",
    "webdriver": "webdriver-manager update"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "camel-case": "^3.0.0",
    "cheerio": "^1.0.0-rc.2",
    "dedent": "^0.7.0",
    "express": "^4.16.4",
    "express-http-proxy": "^1.4.0",
    "fake-xml-http-request": "^1.6.0",
    "fetch-mock": "^5.13.1",
    "pascal-case": "^2.0.1",
    "pem": "^1.13.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "peerDependencies": {
    "@tractor/core": "^1.0.1-beta.10"
  },
  "devDependencies": {
    "@tractor/config-loader": "^1.0.1-beta.10",
    "@tractor/error-handler": "^1.0.1-beta.10",
    "@tractor/file-structure": "^1.0.1-beta.10",
    "@tractor/logger": "^1.0.1-beta.10",
    "@tractor/plugin-loader": "^1.0.1-beta.10",
    "@tractor/ui": "^1.0.1-beta.10",
    "@tractor/unit-test": "^1.0.1-beta.10",
    "@tractor-plugins/browser": "^0.2.5",
    "@tractor-plugins/mocha-specs": "^0.2.5",
    "@tractor-plugins/mock-requests": "^0.4.3",
    "@tractor-plugins/page-objects": "^0.7.2"
  }
}
